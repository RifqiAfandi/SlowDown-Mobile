{
  "expo": {
    "name": "SlowDown-App",
    "slug": "SlowDown-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["android"],
    "android": {
      "package": "com.slowdownapp",
      "adaptiveIcon": {
        "backgroundColor": "#4A90D9"
      },
      "permissions": [
        "PACKAGE_USAGE_STATS",
        "SYSTEM_ALERT_WINDOW"
      ],
      "googleServicesFile": "./android/app/google-services.json"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
